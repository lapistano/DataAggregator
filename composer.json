{
    "name": "liip/dataaggregator",
    "description": "The data aggregator cumulates information provided by a loader and routes them to a persistence layer",
    "type" : "library",
    "license": "MIT",
    "keywords": [ "liip", "data aggregation", "data filtering" ],
    "authors": [
        {
            "name": "Bastian Feder",
            "email": "bastian.feder@liip.ch",
            "role": "maintainer"
        },
        {
            "name": "List of contributors",
            "homepage": "https://github.com/liip/DataAggregator/graphs/contributors"
        }
    ],
    "require": {
        "beberlei/assert": ">=1.5",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "lapistano/proxy-object": "dev-master",
        "phpunit/phpunit" : "@stable"
    },
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "Liip\\DataAggregator": "src" }
    }
}
